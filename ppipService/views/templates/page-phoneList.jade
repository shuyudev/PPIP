md-content(flex)
  md-list
    md-item(ng-repeat="phone in phones")
      md-item-content
        .md-tile-left
          b {{phone.id}}
        .md-tile-content(style="margin-left: 20px")
          h3 {{phone.name}}
          h4 {{phone.token}} / {{phone.deviceName}} / {{phone.status}}
        .md-tile-right(style="margin-right: 20px")
          md-button.md-raised.md-warn(ng-click="delete(phone)") Delete